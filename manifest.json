{
    "manifest_version": 2,
    "name": "Youtube Ad Skip",
    "description": "Skips YT Ads",
    "version": "1.0.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "run_at": "document_idle",
        "js": ["Content.js"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}